<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Trail.
    -->
    <changeSet id="20210110000024-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="trail_id"
                                 baseTableName="trail_tag"
                                 constraintName="fk_trail_tag_trail_id"
                                 referencedColumnNames="id"
                                 referencedTableName="trail"/>

        <addForeignKeyConstraint baseColumnNames="tag_id"
                                 baseTableName="trail_tag"
                                 constraintName="fk_trail_tag_tag_id"
                                 referencedColumnNames="id"
                                 referencedTableName="comments"/>
        
    </changeSet>
</databaseChangeLog>
